syntax = "proto3";

package api;

option go_package = "api";

import "google/api/annotations.proto";

service {{ .Name  }} {
  rpc EchoMessage(Echo) returns (Echo) {
    option (google.api.http) = {
      post: "{{ .HTTPRoutePrefix }}/echo"
      body: "*"
    };
  }
}

message Echo {
  string msg = 1;
}
